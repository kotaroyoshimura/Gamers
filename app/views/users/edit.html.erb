ここはユーザー情報編集ページです</br>
<%= form_with model:@user,local:true do |f| %>
<%= f.text_field :name, autofocus: true %></br>
<%= f.text_area :introduction, autofocus: true %></br>
<%= f.attachment_field :profile_image %></br>
<%= f.email_field :email,autofocus: true %><br>
<%= f.submit "保存"%></br>
<%= link_to "退会",user_unsubscribe_path %>
<% end %>